/*题头开始*/
#BlueTopImage {/*题头大层*/
	height: 188px;
	width: 857px;
	background-image: url(../images/BlueTopImage.jpg);
	positoin:relative;
}
#LoginAndRegister{/*子层用于给登陆注册钮占位的大长方形，与Flash Div、Navigation平级*/
	height: 133px;
	width: 590px;
	padding-top: 10px;
	padding-left: 10px;
	color: #FFF;
	float:left;
	postion:absolute;
	left:0px;
	top:0px;
}
.loginAction {/*登陆注册层里的链接*/
	font-size: 14px;
	text-decoration: underline;
	font-family: Arial, Helvetica, sans-serif;
	color: #FFF;
}
.registerAction {/*登陆注册层里链接*/
	font-size: 14px;
	text-decoration: underline;
	font-family: Arial, Helvetica, sans-serif;
	color: #FFF;
}
#LoginSucc{/*登陆注册层里欢迎*/
	font-size: 14px;
	font-family: Arial, Helvetica, sans-serif;
	color: #CCC;
	float:left;
}
#signoutDiv{/*登陆注册层里退出层*/
	width:100px;
	float:left;
}
.SignOutTop{/*退出层里退出字体*/
	font-size: 14px;
	text-decoration: underline;
	font-family: Arial, Helvetica, sans-serif;
	color: #CCC;
}
#Flash {
	height:134px;
	width:250px;
	float:left;
	position:absolute;
	top:0px;
	left:680px;
}
#navigation    {
	height: 40px;
	width: 700px;
	margin-left: 48px;
	float:left;
}
/*题头结束*/
/*第二部分开始*/
#PictureChange {/*第二部分大层，层内子层有多层叠加*/
	height: 177px;
	width: 857px;
	position: relative;
}
#BandPictureLeft {/*第二部分子层蓝底与大鸟*/
	position: absolute;
	height: 172px;
	width: 243px;
	visibility: visible;
	z-index: 5;
	top: 5px;
	left: 78px;
	overflow: visible;
	float: none;
}
#BigText{/*白道里每个页面不同的大字*/
	position:absolute;
	height: 60px;
	width: 282px;
	visibility: visible;
	z-index: 10;
	top: 42px;
	left: 330px;
	overflow: visible;
	float: none;
}
#Bandpicture    {/*图片轮换*/
	width: 453px;
	height: 172px;
	float: right;
	visibility: visible;
	overflow: visible;
	z-index: -5;
}
/*第二部分结束*/
