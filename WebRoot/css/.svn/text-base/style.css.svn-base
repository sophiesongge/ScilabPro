@charset "utf-8";
/* CSS Document */
/* Download by http://www.codefans.net */
/*Picture Start*/
img {border:none}
.fcnt{
	display:block;
	height:172px;
	width:453px;
	position:relative;
	overflow:hidden;
	right: 78px;
	top: 5px;
}
.fcnt .mimg{ float:left; width:453px; height:172px; overflow:hidden;}
.fcnt .mimg div{ display:none;}
.mimg img{ width:453px; height:172px;} 
.fcnt ul{ float:right; width:152px; height:340px;}
*html .fcnt ul{ position:absolute; right:0; top:0; width:152px; height:340px;}
.fcnt li{ display:block; height:82px; width:150px; margin-bottom:5px; cursor:pointer; overflow:hidden;}
.fcnt li img{ width:150px; height:82px;filter:alpha(opacity=70);  -moz-opacity: 0.7; opacity: 0.7;   }
.fcnt li.cur img{ width:142px; height:75px; margin:4px 0 0 4px;filter:alpha(opacity=100);  -moz-opacity: 1; opacity: 1;  }
/*Picture Stop*/
body{/*全局灰色背景*/
	background:#999999;
	width:857px;
	top: 0px;
	bottom:0px;
	margin: auto;
}
#global{/*全局白色背景*/
	width:857px;
	background-color:#fff;
	height:auto!important; min-height:600px; height:600px; 
	clear:both;
}
#globalIndex{/*全局白色背景*/
	width:857px;
	background-color:#fff;
	height:1600px;
	clear:both;
}
